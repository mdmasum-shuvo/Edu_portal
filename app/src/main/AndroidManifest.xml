<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.masum.edu_portal">

    <application
        android:name=".common.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".feature.member.view.MyProfileActivity"></activity>
        <activity android:name=".feature.myclass.view.MyClassActivity" />
        <activity android:name=".feature.home.view.NotificationActivity" />
        <activity android:name=".feature.home.view.UploadPostActivity" />
        <activity android:name=".feature.launch.view.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".feature.member.view.ProfileActivity" />
        <activity android:name=".feature.home.view.HomeActivity" />
    </application>

</manifest>